{
  "manifest_version": 2,

  "name": "8comic Downloader",
  "description": "This extension demonstrates a 'manga download action'.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",  
	"storage",  
	"downloads",
	"notifications",  
	"unlimitedStorage",  
	"http://*/*"
  ]
}
